{% extends "partials/base.njk" %}

{% block title %}This{% endblock %}

{% set meta_description = "" %}
{% set og_url = "" %}
{% set og_title = "" %}
{% set og_description = "" %}
{% set og_img = "" %}
{% set og_img_width = "" %}
{% set og_img_height = "" %}

{% set view_name = "templateb" %}

{% block content %}
<section class="templateb vre">
  <section class="templateb-cover">
    <div class="return-container">
      <a class="return metropolis-semibold" href="{{tbES.base.linkReturn}}"><img class="semi-triangle" src="{{tbES.base.iconReturn}}" alt="{{tbES.base.iconReturnAlt}}">{{tbES.base.textReturn}}</a>
    </div>
    <div class="templateb-cover-content">
      <img class="templateb-logo" src="{{tbES.vre.cover.projectLogo}}" alt="{{tbES.vre.cover.projectLogoAlt}}">
      <h1 class="templateb-cover-title metropolis-bold">{{tbES.vre.cover.projectName}}&nbsp;<span class="metropolis-medium">|&nbsp;{{tbES.vre.cover.projectType}}</span></h1>
      <p class="templateb-cover-paragraph metropolis-medium">{{tbES.vre.cover.projectDesc}}</p>
    </div>
  </section>

  <section class="templateb-data">
    <img class="templateb-data-img" src="{{tbES.vre.data.laptopImg}}" alt="{{tbES.vre.data.laptopImgAlt}}">
    <div class="templateb-data-container">
      <div>
        <h2 class="templateb-data-title metropolis-semibold">{{tbES.base.data.title}}</h2>
        <span class="templateb-data-desc metropolis-light"><span class="metropolis-medium">{{tbES.base.data.textYear}}</span>&nbsp;{{tbES.vre.data.numberYear}}</span>
        <ul class="templateb-data-ul">
          {% for list in tbES.vre.data.functionalities %}
          <li class="templateb-data-desc metropolis-light">{% if list.title %}<span class="metropolis-medium">{{list.title}}</span>&nbsp;{% endif %}{{list.item}}</li>
          {% endfor %}
        </ul>
        <span class="templateb-data-desc metropolis-light"><span class="metropolis-medium">{{tbES.base.data.locationTitle}}</span>&nbsp;{{tbES.vre.data.location}}</span>
      </div>
      <div class="templateb-data-btns">
        <a class="btn borderBlack metropolis-bold" href="{{tbES.base.data.btnSeeLink}}">{{tbES.base.data.btnSee}}</a>
        <a class="btn metropolis-bold" href="{{tbES.base.data.btnQuoteLink}}">{{tbES.base.data.btnQuote}}</a>
      </div>
    </div>
  </section>
</section>
{% endblock %}
