{% extends "partials/base.njk" %}

{% block title %}Website of Wicore{% endblock %}

{% set meta_description = "Blog integration | Contact and Location | Animation and effects | Social Media Links | Web and Mobile Design | Video integration" %}
{% set og_url = "https://getmore.mx/en/our-work/wicore.html" %}
{% set og_title = "Website of Wicore | Get More" %}
{% set og_description = "Blog integration | Contact and Location | Animation and effects | Social Media Links | Web and Mobile Design | Video integration" %}
{% set og_img = "/img/seo/pagina-web-seo.jpg" %}
{% set og_img_width = "500" %}
{% set og_img_height = "300" %}

{% set view_name = "ourwork" %}
{% set lang_view = "english" %}

{% block content %}
<section class="templateb wicore">
  <section class="templateb-cover">
    <div class="return-container">
      <a class="return metropolis-semibold" href="{{tbEN.base.linkReturn}}"><img class="semi-triangle" src="{{tbEN.base.iconReturn}}" alt="{{tbEN.base.iconReturnAlt}}">{{tbEN.base.textReturn}}</a>
    </div>
    <div class="templateb-cover-content">
      <img class="templateb-logo" src="{{tbEN.wicore.cover.projectLogo}}" alt="{{tbEN.wicore.cover.projectLogoAlt}}">
      <h1 class="templateb-cover-title metropolis-bold">{{tbEN.wicore.cover.projectName}}&nbsp;<span class="metropolis-medium">|&nbsp;{{tbEN.wicore.cover.projectType}}</span></h1>
      <p class="templateb-cover-paragraph metropolis-medium">{{tbEN.wicore.cover.projectDesc}}</p>
    </div>
  </section>

  <section class="templateb-data">
    <img class="templateb-data-img" src="{{tbEN.wicore.data.laptopImg}}" alt="{{tbEN.wicore.data.laptopImgAlt}}">
    <div class="templateb-data-container">
      <div>
        <h2 class="templateb-data-title metropolis-semibold">{{tbEN.base.data.title}}</h2>
        <span class="templateb-data-desc metropolis-light"><span class="metropolis-medium">{{tbEN.base.data.textYear}}</span>&nbsp;{{tbEN.wicore.data.numberYear}}</span>
        <ul class="templateb-data-ul">
          {% for list in tbEN.wicore.data.functionalities %}
          <li class="templateb-data-desc metropolis-light">{% if list.title %}<span class="metropolis-medium">{{list.title}}</span>&nbsp;{% endif %}{{list.item}}</li>
          {% endfor %}
        </ul>
        <span class="templateb-data-desc metropolis-light"><span class="metropolis-medium">{{tbEN.base.data.locationTitle}}</span>&nbsp;{{tbEN.wicore.data.location}}</span>
      </div>
      <div class="templateb-data-btns">
        <a class="btn borderBlack metropolis-bold" href="{{tbEN.wicore.data.btnSeeLink}}" target="_blank">{{tbEN.base.data.btnSee}}</a>
        <a class="btn metropolis-bold" href="{{tbEN.base.data.btnQuoteLink}}">{{tbEN.base.data.btnQuote}}</a>
      </div>
    </div>
  </section>
</section>
{% endblock %}
